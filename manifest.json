{
  "name": "Wat Dat?",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A Chrome extension to quickly Google an image.",
  "homepage_url": "http://github.com/ChrisMissal/watdat",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "contextMenus", "tabs"
  ],
  "background": {
    "scripts": [ "js/menu.js" ]
  }
}